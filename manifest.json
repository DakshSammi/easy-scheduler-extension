{
	"name": "Easy Scheduler",
	"version": "2",
	"description": "Adds the functionality of obtaining the best deadline release and due dates based on student workload of other courses.",
	"manifest_version": 2,
	"background":{
        "persistent":true,
        "page":"backpack-background.html"
	},
	"content_security_policy":"script-src 'self' https://www.gstatic.com https://unpkg.com https://cdn.jsdelivr.net; object-src 'self'",
	"permissions":[
        "https://run.mocky.io/",
        "tabs"
    ],
	"content_scripts": [
	    {
	      "matches": ["https://classroom.google.com/*"],
	      "js": ["js/globals.js", "js/classroom/suggestions.js", "js/classroom/get_suggestions.js", "js/classroom/suggestions_popup.js", "js/classroom/free_alert.js", "js/classroom/inform_about_quiz.js", "js/classroom/quiz_detection.js", "js/classroom/course_name_change.js", "js/classroom/main.js"],
	      "css": ["css/classroom-styles.css"]
		},
		{   
            "css":["./css/backpack-styles.css"],
            "js":["js/backpack/content.js", "js/backpack/background.js"],
            "matches":["http://*/*", "https://*/*"],
            "run_at": "document_end"
        }
	  ],
	"web_accessible_resources": ["assets/bell.gif", "assets/loading.gif"]
}